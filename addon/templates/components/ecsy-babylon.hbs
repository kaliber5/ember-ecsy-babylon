<canvas id="{{this.guid}}__canvas" {{did-insert this.onCanvasReady}} ...attributes></canvas>

{{#if this.entity}}
  {{yield
    (component "ecsy-babylon/scene"
      parent=this
      w=(hash
        world=this.world
        private=(hash
          rootEntity=this.entity
          createEntity=this.createEntity
          componentReference=this
        )
      )
    )
  }}
{{/if}}