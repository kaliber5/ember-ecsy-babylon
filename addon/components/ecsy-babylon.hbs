<canvas
  id="{{this.guid}}__canvas"
  {{did-insert this.onCanvasReady}}
  {{on "keypress" this.toggleBabylonInspector}}
  ...attributes>
{{#if (has-block "ecsy")}}
  {{#if this.ready}}
    {{yield
      (component "ecsy-babylon/scene"
        parent=this
      )
      to="ecsy"
    }}
  {{/if}}
  {{yield to="html"}}
{{else}}
  {{#if this.ready}}
    {{yield
      (component "ecsy-babylon/scene"
        parent=this
      )
    }}
  {{/if}}
{{/if}}

</canvas>
