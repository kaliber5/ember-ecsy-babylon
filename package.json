{
  "name": "ember-ecsy-babylon",
  "version": "0.6.0",
  "description": "Ember addon that uses escy and babylon.js for a fully declarative 3D rendering experience in Ember apps",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/kaliber5/ember-ecsy-babylon.git",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint . --ext .js,.ts",
    "lint:ts": "tsc --noEmit",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@babylonjs/core": "^4.1.0",
    "@babylonjs/inspector": "^4.1.0",
    "@babylonjs/loaders": "^4.1.0",
    "@ember/render-modifiers": "^1.0.2",
    "@glimmer/env": "^0.1.7",
    "@glimmer/tracking": "^1.0.2",
    "ecsy": "^0.4.2",
    "ecsy-babylon": "^0.6.1",
    "ember-auto-import": "^1.6.0",
    "ember-cli-babel": "^7.23.0",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-typescript": "^4.0.0",
    "ember-concurrency": "^1.3.0",
    "ember-concurrency-decorators": "^2.0.1",
    "ember-concurrency-ts": "^0.2.0",
    "ember-destroyable-polyfill": "^2.0.2"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "^1.0.2",
    "@glimmer/tracking": "^1.0.2",
    "@types/ember": "^3.16.1",
    "@types/ember-qunit": "^3.4.12",
    "@types/ember__test-helpers": "^1.7.3",
    "@types/qunit": "^2.9.5",
    "@types/rsvp": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.21.2",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-css-modules": "^1.3.3",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-get-config": "^0.2.4",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.21.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.14.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.11.0",
    "eslint-plugin-ember": "^9.3.0",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "pretender": "^3.4.3",
    "qunit-dom": "^1.5.0",
    "release-it": "^14.2.0",
    "release-it-lerna-changelog": "^2.4.0",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "volta": {
    "node": "10.22.0",
    "yarn": "1.22.4"
  }
}
