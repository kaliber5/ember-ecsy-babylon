{
  "name": "ember-ecsy-babylon",
  "version": "0.2.5",
  "description": "Ember addon that uses escy and babylon.js for a fully declarative 3D rendering experience in Ember apps",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/kaliber5/ember-ecsy-babylon.git",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint:ts": "tsc --noEmit",
    "lint:js": "eslint . --ext .js,.ts",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean"
  },
  "dependencies": {
    "@babylonjs/core": "^4.1.0",
    "@babylonjs/loaders": "^4.1.0",
    "@ember/render-modifiers": "^1.0.2",
    "@glimmer/env": "^0.1.7",
    "@glimmer/tracking": "^1.0.0",
    "ecsy": "^0.2.6",
    "ecsy-babylon": "^0.1.9",
    "ember-auto-import": "^1.5.3",
    "ember-cli-babel": "^7.21.0",
    "ember-cli-htmlbars": "^5.2.0",
    "ember-cli-typescript": "^3.1.4",
    "ember-concurrency": "^1.2.1",
    "ember-concurrency-decorators": "^1.1.0-alpha.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "@types/ember": "^3.16.0",
    "@types/ember-qunit": "^3.4.9",
    "@types/ember__test-helpers": "^0.7.12",
    "@types/qunit": "^2.9.1",
    "@types/rsvp": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.17.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-css-modules": "^1.3.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-get-config": "^0.2.4",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.0",
    "ember-source": "~3.17.3",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.9.0",
    "ember-try": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-ember": "^8.7.0",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "pretender": "^3.4.3",
    "qunit-dom": "^1.2.0",
    "typescript": "3.9.5"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
